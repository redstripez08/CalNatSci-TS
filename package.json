{
  "name": "calnatsci-ts",
  "version": "2.0.0",
  "description": "CalNatSci Bot rewritten in TypeScript",
  "main": "./dist/index.js",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node .",
    "dev": "node -r dotenv/config .",
    "ts-dev": "ts-node-dev -r dotenv/config ./src/index.ts",
    "ts:watch": "tsc --watch",
    "ts-node": "ts-node-dev",
    "test": "jest",
    "heroku-postbuild": "tsc && npm --prefix CalNatSci-TS-server ci",
    "heroku-cleanup": "npm --prefix CalNatSci-TS-server prune --production"
  },
  "engines": {
    "node": "^14.15.1",
    "npm": "^7.18.1"
  },
  "types": "typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redstripez08/CalNatSci-TS.git"
  },
  "author": "Angelo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/redstripez08/CalNatSci-TS/issues"
  },
  "homepage": "https://github.com/redstripez08/CalNatSci-TS#readme",
  "dependencies": {
    "@keyv/mongo": "^1.1.0",
    "@prisma/client": "^2.27.0",
    "axios": "^0.21.1",
    "discord.js": "^12.5.3",
    "keyv": "^4.0.3",
    "luxon": "^2.0.1",
    "prisma": "^2.27.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/keyv": "^3.1.1",
    "@types/luxon": "^1.27.1",
    "@types/node": "^14.17.5",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.30.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.8",
    "typedoc": "^0.21.4",
    "typescript": "^4.3.5"
  }
}
