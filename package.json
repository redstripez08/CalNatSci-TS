{
  "name": "calnatsci-ts",
  "version": "1.1.2",
  "description": "CalNatSci Bot rewritten in TypeScript",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node .",
    "dev:start": "node --trace-warnings -r dotenv/config .",
    "dev:watch": "nodemon --trace-warnings -r dotenv/config .",
    "server:start": "node ./dist/server/index.js",
    "server:dev": "node -r dotenv/config ./dist/server/index.js",
    "ts:start": "ts-node -r dotenv/config ./src/index.ts",
    "ts:watch": "tsc --watch"
  },
  "engines": {
    "node": "^14.15.1",
    "npm": "^7.5.2"
  },
  "types": "typings/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redstripez08/CalNatSci-TS.git"
  },
  "author": "Angelo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/redstripez08/CalNatSci-TS/issues"
  },
  "homepage": "https://github.com/redstripez08/CalNatSci-TS#readme",
  "dependencies": {
    "@keyv/mongo": "^1.1.0",
    "@prisma/client": "^2.23.0",
    "axios": "^0.21.1",
    "discord.js": "^12.5.1",
    "express": "^4.17.1",
    "keyv": "^4.0.3",
    "luxon": "^1.26.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/keyv": "^3.1.1",
    "@types/luxon": "^1.26.0",
    "@types/node": "^14.14.25",
    "dotenv": "^8.2.0",
    "prisma": "^2.23.0",
    "typescript": "^4.2.3"
  },
  "_globalDependencies": {
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1"
  }
}
